{
  "compilerOptions": {
      "declaration": true,
      "esModuleInterop": true,
      "inlineSourceMap": false,
      "lib": [
          "dom",
          "es7"
      ],
      "module": "commonjs",
      "moduleResolution": "node",
      "noFallthroughCasesInSwitch": true,
      "noImplicitAny": false,
      "outDir": "./lib",
      "removeComments": false,
      "resolveJsonModule": true,
      "sourceMap": true,
      "target": "es5",
      "incremental": true
  },
  "exclude": [
      ".vscode-test",
      "lib",
      "node_modules"
  ]
}
